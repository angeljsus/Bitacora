.lista-faltantes
	width 100%
	height 100%
	overflow auto
	display flex
	flex-wrap wrap
	justify-content space-evenly
	align-content flex-start
	border 1px solid rgba(0,0,0,.2)
	.lista-mes
		width 170px
		height 30px
		display flex
		justify-content center
		align-items center
		margin 5px
		border 1px solid
		font-weight bold
		font-size .7em
		cursor context-menu
		border 2px solid rgba(0,0,0,.2)
		&:hover
			background #48cae4
			font-weight bold
			color #FFFF
